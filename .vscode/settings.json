{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "eslint.useFlatConfig": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
    // "source.fixAll.eslint": "always"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml",
    "toml"
  ],
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
